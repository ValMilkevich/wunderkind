<h1>New rate</h1>

<% form_for(@rate) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :ratable_id %><br />
    <%= f.text_field :ratable_id %>
  </p>
  <p>
    <%= f.label :ratable_type %><br />
    <%= f.text_field :ratable_type %>
  </p>
  <p>
    <%= f.label :value %><br />
    <%= f.text_field :value %>
  </p>
  <p>
    <%= f.label :notes %><br />
    <%= f.text_field :notes %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', rates_path %>
